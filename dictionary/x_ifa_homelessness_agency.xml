<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Agency" max_length="40" name="x_ifa_homelessness_agency" type="collection">
        <element display="true" label="Agency Name" mandatory="true" max_length="200" name="agencyname" type="string"/>
        <element label="City" max_length="50" name="agency_city" type="string"/>
        <element active="false" label="agency_state_abbrv" max_length="2" name="agency_state_abbrv" type="string"/>
        <element label="Street Address" max_length="100" name="agency_street_address" type="string"/>
        <element label="Zip Code" max_length="10" name="agency_zip_code" type="string"/>
        <element label="Contract Number" max_length="50" name="contract_number" type="string"/>
        <element label="Data Collection System" max_length="50" name="data_collection_system" type="string"/>
        <element label="Legal Name" max_length="400" name="legal_name" type="string"/>
        <element choice="1" default="IA" label="State" max_length="32" name="state_location" reference="u_state_type" type="reference">
            <choice/>
        </element>
        <element label="DUNS Number" max_length="20" name="u_duns_number" type="string"/>
        <index name="index">
            <element name="state_location"/>
        </index>
    </element>
</database>
