<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Agency Draw" max_length="40" name="x_ifa_homelessness_agency_draw" type="collection">
        <element label="Agency Award" max_length="32" name="agency_award" read_only="true" reference="x_ifa_homelessness_agency_award" type="reference"/>
        <element label="Approved In IDIS Date" max_length="40" name="approved_in_idis_date" type="glide_date"/>
        <element label="Draw Amount" mandatory="true" max_length="17" name="draw_amount" type="currency"/>
        <element label="Entered In IDIS Date" max_length="40" name="entered_in_idis_date" type="glide_date"/>
        <element label="IDIS Voucher Number" max_length="20" name="idis_voucher_number" reference="x_ifa_homelessness_idis_voucher" type="reference"/>
        <element label="Management Approval Date" max_length="40" name="management_approval_date" type="glide_date"/>
        <element default="javascript:global.getNextObjNumberPadded();" display="true" label="Draw Number" max_length="40" name="number" read_only="true" type="string"/>
        <element choice="1" label="Program Fund Year" mandatory="true" max_length="32" name="program_fund_year" reference="x_ifa_homelessness_program_fund_year" reference_qual="javascript:ref_qual_get_program_code();" type="reference" use_dependent_field="true" use_reference_qualifier="advanced">
            <choice/>
        </element>
