        <element label="Sent To Accounting Date" max_length="40" name="sent_to_accounting_date" type="glide_date"/>
        <element label="Sequence Number" max_length="40" name="sequence_number" read_only="true" type="integer"/>
        <element label="Contact Email" max_length="100" name="u_contact_email" type="string"/>
        <element label="Contact Name" max_length="100" name="u_contact_name" type="string"/>
        <element label="Contact Phone" max_length="20" name="u_contact_phone" type="string"/>
        <element label="Draw Notes" max_length="4000" name="u_draw_notes" type="journal"/>
        <element label="Item Total Amount" max_length="40" name="u_item_total_amount" read_only="true" type="currency"/>
        <element choice="1" label="Reporting Months" mandatory="true" max_length="4000" name="u_reporting_months" type="glide_list">
            <choice>
                <element label="January" sequence="1" value="January"/>
                <element label="February" sequence="2" value="February"/>
                <element label="March" sequence="3" value="March"/>
                <element label="April" sequence="4" value="April"/>
                <element label="May" sequence="5" value="May"/>
                <element label="June" sequence="6" value="June"/>
                <element label="July" sequence="7" value="July"/>
                <element label="August" sequence="8" value="August"/>
                <element label="September" sequence="9" value="September"/>
                <element label="October" sequence="10" value="October"/>
                <element label="November" sequence="11" value="November"/>
                <element label="December" sequence="12" value="December"/>
            </choice>
        </element>
        <index name="index">
            <element name="agency_award"/>
        </index>
        <index name="index2">
            <element name="program_fund_year"/>
        </index>
    </element>
</database>
