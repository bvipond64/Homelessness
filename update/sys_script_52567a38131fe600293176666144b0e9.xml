        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	//gs.addInfoMessage('Rule Started');

	// Set the Contract Submission deadline
	var gr = new GlideRecord('x_ifa_homelessness_program_fund_year');
	gr.addQuery('fund_year_ref.u_year',current.grant_year_ref.u_year);
	gr.addQuery('program.program_code',current.program_code.program_code);
	gr.query();
	
	current.contract_submission_deadline = '';

	while (gr.next()){
		//gs.addInfoMessage('contract sub dead = ' + gr.contract_submission_deadline);
		current.contract_submission_deadline = gr.contract_submission_deadline;
	}
	
	current.update();

})(current, previous);]]></script>
