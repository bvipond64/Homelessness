        <script><![CDATA[function ref_qual_get_agency_draw() {
	
	//gs.addInfoMessage(current.agency_draw.agency_award.program_code.program_code);
	
	var result = 'sys_idIN';
	var ids1 = [];
	
	var m_item = new GlideRecord('x_ifa_homelessness_agency_draw');  
	m_item.addQuery('agency_award', current.agency_match.agency_award); 
	m_item.query(); 
	
	while (m_item.next()) {  
		ids1.push('' + m_item.sys_id);
	}	
	
	//gs.addInfoMessage(ids1.toString());
	return result + ids1.toString();
}]]></script>
